    "torrents-worker": "node dist/src/worker/TorrentsWorker/index.js",
    "s": "nodemon src/index.ts",
    "sw": "nodemon src/worker/index.ts",
    "stw": "nodemon src/worker/TorrentsWorker/index.ts",
    "lint": "eslint . --ext .ts",
    "test": "jest",
    "postinstall": "chmod +x ./postinstall && ./postinstall",
